{
  "author": "Anne Steenbeek",
  "name": "quickplot",
  "version": "0.0.1",
  "description": "A super simple plotting tool used for debugging microcontrollers like arduinos",
  "main": "src/index.html",
  "dependencies": {
    "serialport": "*",
    "node-webkit-fdialogs": "^0.2.7"
  },
  "window": {
    "title": "Quickplot",
    "width": 985,
    "min-width": 797,
    "height": 600,
    "toolbar": false,
    "frame": true,
    "resizable": true,
    "icon": "src/favicon.png"
  },
  "scripts": {
    "start": "nw"
  },
  "devDependencies": {
    "grunt": "^0.4.2",
    "grunt-nw-builder": "2.0.0",
    "grunt-wiredep": "^2.0.0",
    "nw": "0.12.0"
  }
}
